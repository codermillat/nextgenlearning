{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "studyinindiabd.vercel.app"
        }
      ],
      "destination": "https://www.nextgenlearning.dev/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.studyinindiabd.vercel.app"
        }
      ],
      "destination": "https://www.nextgenlearning.dev/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "nextgenlearning.dev"
        }
      ],
      "destination": "https://www.nextgenlearning.dev/:path*",
      "permanent": true
    },
    {
      "source": "/categories/:path*",
      "destination": "/program-categories",
      "permanent": true
    },
    {
      "source": "/tags/:path*",
      "destination": "/guides",
      "permanent": true
    },
    {
      "source": "/tags/:path*/",
      "destination": "/guides",
      "permanent": true
    },
    {
      "source": "/posts/:path*",
      "destination": "/guides",
      "permanent": true
    },
    {
      "source": "/sharda",
      "destination": "/sharda-university",
      "permanent": true
    },
    {
      "source": "/universities/sharda-university",
      "destination": "/sharda-university",
      "permanent": true
    },
    {
      "source": "/universities/sharda-university/",
      "destination": "/sharda-university",
      "permanent": true
    },
    {
      "source": "/sharda/",
      "destination": "/sharda-university",
      "permanent": true
    },
    {
      "source": "/sharda/admissions",
      "destination": "/apply",
      "permanent": true
    },
    {
      "source": "/sharda/rankings",
      "destination": "/sharda-university/ranking-2026",
      "permanent": true
    },
    {
      "source": "/sharda/fees",
      "destination": "/sharda-university/btech-cse-fees",
      "permanent": true
    },
    {
      "source": "/sharda/programs",
      "destination": "/sharda-university/programs/btech-cse",
      "permanent": true
    },
    {
      "source": "/sharda/programs/:program",
      "destination": "/sharda-university/programs/:program",
      "permanent": true
    },
    {
      "source": "/sharda/nirf-ranking",
      "destination": "/sharda-university/nirf-ranking",
      "permanent": true
    },
    {
      "source": "/sharda/ranking-2026",
      "destination": "/sharda-university/ranking-2026",
      "permanent": true
    },
    {
      "source": "/sharda/btech-cse-fees",
      "destination": "/sharda-university/btech-cse-fees",
      "permanent": true
    },
    {
      "source": "/sharda/mba-fees",
      "destination": "/sharda-university/mba-fees",
      "permanent": true
    },
    {
      "source": "/sharda/scholarship-bangladeshi-students",
      "destination": "/sharda-university/scholarship-bangladeshi-students-india",
      "permanent": true
    },
    {
      "source": "/sharda/scholarship-bangladeshi-students-india",
      "destination": "/sharda-university/scholarship-bangladeshi-students-india",
      "permanent": true
    },
    {
      "source": "/sharda/study-in-india-from-bangladesh",
      "destination": "/sharda-university/study-in-india-from-bangladesh",
      "permanent": true
    },
    {
      "source": "/sharda/indian-university-bangladeshi-students",
      "destination": "/sharda-university/indian-university-bangladeshi-students",
      "permanent": true
    },
    {
      "source": "/sharda/best-universities-bangladeshi-students",
      "destination": "/best-universities-bangladeshi-students-india",
      "permanent": true
    },
    {
      "source": "/linear-algebra-for-ml",
      "destination": "/guides",
      "permanent": true
    },
    {
      "source": "/linear-algebra-for-ml/",
      "destination": "/guides",
      "permanent": true
    },
    {
      "source": "/lora-vs-full-fine-tuning",
      "destination": "/guides",
      "permanent": true
    },
    {
      "source": "/lora-vs-full-fine-tuning/",
      "destination": "/guides",
      "permanent": true
    },
    {
      "source": "/lora-fine-tuning-explained",
      "destination": "/guides",
      "permanent": true
    },
    {
      "source": "/lora-fine-tuning-explained/",
      "destination": "/guides",
      "permanent": true
    },
    {
      "source": "/privacy-policy/",
      "destination": "/privacy-policy",
      "permanent": true
    },
    {
      "source": "/terms-and-conditions/",
      "destination": "/terms-and-conditions",
      "permanent": true
    },
    {
      "source": "/2025/:path*",
      "destination": "/guides",
      "permanent": true
    },
    {
      "source": "/2024/:path*",
      "destination": "/guides",
      "permanent": true
    },
    {
      "source": "/search/:path*",
      "destination": "/courses",
      "permanent": true
    },
    {
      "source": "/search",
      "destination": "/courses",
      "permanent": true
    },
    {
      "source": "/2025/09/:path*",
      "destination": "/guides",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow, max-image-preview:large, max-snippet:-1, max-video-preview:-1"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Resource-Policy",
          "value": "same-origin"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://www.googletagmanager.com https://www.google-analytics.com https://cdn.jsdelivr.net https://va.vercel-scripts.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; font-src 'self' https://fonts.gstatic.com; img-src 'self' data: https: blob:; connect-src 'self' https://www.google-analytics.com https://www.googletagmanager.com https://region1.google-analytics.com https://vitals.vercel-insights.com; frame-src 'self' https://www.google.com; object-src 'none'; base-uri 'self'; form-action 'self' https://wa.me; frame-ancestors 'none'; upgrade-insecure-requests;"
        }
      ]
    },
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    },
    {
      "source": "/(.*).js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).webp",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).jpg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).svg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
